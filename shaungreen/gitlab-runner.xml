<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Gitlab Runner</Name>
  <Description>Description</Description>
  <Registry>https://hub.docker.com/r/shaungreen/unraid-gitlab-runner/</Registry>
  <Repository>shaungreen/unraid-gitlab-runner/</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2222</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/GitlabRunner/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>05616fd2</Version>
  <WebUI/>
  <Banner/>
  <Icon/>
  <ExtraParams/>
  <Config
    Name="Gitlab CI URL"
    Type="Variable"
    Target="GITLAB_CI_URL"
    Default="https://gitlab.com/ci"
    Description="URL that the runner will be registered to"
    Required="true"
    />
</Container>
