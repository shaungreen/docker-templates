<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Gitlab Runner</Name>
  <Description>Description</Description>
  <Registry>https://hub.docker.com/r/shaungreen/unraid-gitlab-runner/</Registry>
  <Repository>shaungreen/unraid-gitlab-runner/</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2222</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/GitlabRunner/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>05616fd2</Version>
  <WebUI/>
  <Banner/>
  <Icon/>
  <ExtraParams/>
  <Config
    Name="CI URL"
    Type="Variable"
    Target="GITLAB_CI_URL"
    Default="https://gitlab.com/ci"
    Description="URL that the runner will connect to when registering."
    Required="true"
    />
  <Config
    Name="Token"
    Type="Variable"
    Target="GITLAB_CI_TOKEN"
    Description="The token issued by gitlab.com that will is used during runner registration."
    Required="true"
    >token</Config>
  <Config
    Name="Description"
    Type="Variable"
    Target="GITLAB_CI_DESC"
    Default="private unRAID runner"
    Description="The description of this runner that will appear in your pipelines."
    Required="true"
    >description</Config>
  <Config
    Name="Executor"
    Type="Variable"
    Target="GITLAB_CI_EXECUTOR"
    Default="docker"
    Description="e.g. shell, docker, docker-ssh, ssh."
    Required="true"
    />
  <Config
    Name="Docker image"
    Type="Variable"
    Target="GITLAB_CI_IMAGE"
    Default="alpine:latest"
    Description="Default docker image to use if none specified in .gitlab-ci.yml."
    Required="true"
    />
</Container>
